{
  "name": "serverless-playground",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "clean": "rimraf .serverless coverage",
    "deploy": "sls deploy --verbose",
    "package": "sls package --verbose",
    "remove": "sls remove --verbose",
    "start": "sls offline",
    "test": "jest",
    "test:ci": "jest --coverage --silent --verbose --color",
    "test:coverage": "jest --coverage --watch",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "4.7.0",
    "@middy/http-error-handler": "4.7.0",
    "@middy/http-event-normalizer": "4.7.0",
    "@middy/http-json-body-parser": "4.7.0",
    "axios": "1.6.8",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.136",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.17.0",
    "@types/node": "20.11.30",
    "esbuild": "0.20.2",
    "jest": "29.7.0",
    "rimraf": "5.0.5",
    "serverless": "3.38.0",
    "serverless-esbuild": "1.52.1",
    "serverless-offline": "13.3.3",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.4.3"
  },
  "author": "Matthew Warman",
  "license": "MIT"
}
